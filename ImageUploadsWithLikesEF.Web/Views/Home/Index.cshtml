@using ImageUploadsWithLikesEF.Data
@model HomePageViewModel
<div class="row">
    @foreach(Image image in Model.Images){
            <div class="col-md-8 offset-md-2">
        <div class="bg-light p-4 rounded shadow mb-3" style="margin-bottom: 20px; text-align: center;">
            <a href="/home/viewimage?id=@image.Id">
                <h3>@image.Title</h3>
                <h5>Uploaded at:@image.UploadedAt</h5>
                <img style="width: 400px;" src="@image.FileName" />
            </a>
        </div>
        
    </div>
    }
 
</div>
    

<script src="/lib/jquery/dist/jquery.min.js"></script>
<script src="/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
